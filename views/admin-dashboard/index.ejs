    <script>

        $(document).ready(() => {
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/users/total", (data, status) => {
                if(status === "success") {
                    $('#total_users_counter').text(data.total)
                }
            })
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/memes/total", (data, status) => {
                if(status === "success") {
                    $('#total_memes_counter').text(data.total)
                }
            })
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/paygateways/total", (data, status) => {
                if(status === "success") {
                    $('#total_paymentgayway_counter').text(data.total)
                }
            })
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/likes/total", (data, status) => {
                if(status === "success") {
                    $('#total_likes_counter').text(data.total)
                }
            })
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/comments/total", (data, status) => {
                if(status === "success") {
                    $('#total_comments_counter').text(data.total)
                }
            })
            $.get('<%= process.env.APP_URL %>'+"/api/statistics/shares/total", (data, status) => {
                if(status === "success") {
                    $('#total_shares_counter').text(data.total)
                }
            })
        })

    </script>
